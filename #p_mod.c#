/**                                                                                                                                                                                                                 
 * print_mod - Prints modulus                                                                                                                                                                                       
 * @list: list of arguments                                                                                                                                                                                         
 * Return: character amount.                                                                                                                                                                                        
 */                                                                                                                                                                                                                 
int p_mod(va_list list)                                                                                                                                                                                             
{                                                                                                                                                                                                                   
        (void)list;                                                                                                                                                                                                 
        _putchar('%');                                                                                                                                                                                              
        return (1);                                                                                                                                                                                                 
}  